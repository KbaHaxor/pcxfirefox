diff -r bcdfefbd8e50 -r 9257001377a7 media/libvpx/Makefile.in
--- a/media/libvpx/Makefile.in	Wed Sep 24 00:56:22 2014 +0800
+++ b/media/libvpx/Makefile.in	Thu Sep 11 18:00:15 2014 +0900
@@ -8,6 +8,11 @@
 AS_DASH_C_FLAG=$(VPX_DASH_C_FLAG)
 ASM_SUFFIX=$(VPX_ASM_SUFFIX)
 
+ifdef _MSC_VER
+# No macro to detect current VS version by yasm
+ASFLAGS += -D_MSC_VER=$(_MSC_VER)
+endif
+
 ifdef VPX_ARM_ASM
 # Building on an ARM platform with a supported assembler, include
 # the optimized assembly in the build.
diff -r bcdfefbd8e50 -r 9257001377a7 media/libvpx/moz.build
--- a/media/libvpx/moz.build	Wed Sep 24 00:56:22 2014 +0800
+++ b/media/libvpx/moz.build	Thu Sep 11 18:00:15 2014 +0900
@@ -45,6 +45,9 @@
     # Darwin toolchain right now
     if CONFIG['OS_TARGET'] == 'Darwin':
         SOURCES += files['AVX2']
+    # HAVE_X86_AVX2 doesn't check whether AVX2 intrincs is supported.
+    elif CONFIG['_MSC_VER'] and CONFIG['HAVE_X86_AVX2']:
+        SOURCES += files['AVX2']
 
     #postproc is only enabled on x86 with asm
     SOURCES += files['VP8_POSTPROC']
